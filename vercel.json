{
    "builds":[
        {
            "src":"./app.js",
            "use":"@vercel/node"
        }

    ],

    "routes":[
        {
            "src":"/.*",
            "dest":"app.js"
        }

    ],
    
    "rewrites": [
         {
            "source": "/api/(.*)",
            "destination": "https://jiomusicbackend.onrender.com"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
